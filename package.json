{
  "name": "home_together",
  "version": "1.0.0",
  "description": "A SmartHome demo using craft ai",
  "author": "craft ai <contact@craft.ai>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craft-ai/learning.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/main.js",
    "build_static": "cp ./src/static/index.html ./bin/static",
    "build_server": "babel ./src/server --out-dir ./bin",
    "build_browser": "webpack",
    "build": "npm run build_static && npm run build_server && npm run build_browser",
    "dev": "cross-env NODE_ENV=development npm run build_static && npm run build_server && node ./bin/main.js",
    "watch": "nodemon --exec 'npm run dev'",
    "lint": "eslint ."
  },
  "license": "BSD-3-Clause",
  "homepage": "https://craft.ai/HomeTogether",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.4.0",
    "eslint-config-craft-ai": "1.0.1",
    "eslint-plugin-react": "4.2.3",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "nodemon": "1.9.1",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.5.1"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "body-parser": "1.15.0",
