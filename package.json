    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.6.1",
    "node-hue-api": "2.1.1",
    "passport": "0.3.2",
    "passport-oauth": "1.0.0",
    "react": "0.14.7",
    "react-bootstrap": "0.28.3",
    "react-dom": "0.14.7",
    "react-motion": "0.4.2",
    "webpack": "1.12.14",
    "ws": "1.0.1"
  }
