{
  "name": "home_together",
  "version": "1.0.0",
  "description": "A SmartHome demo using craft ai",
  "author": "craft ai <contact@craft.ai>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craft-ai/learning.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/main.js",
    "build_static": "cpy ./src/static/index.html ./bin/static",
    "build_server": "babel ./src/server --out-dir ./bin",
    "build_browser": "webpack",
    "build": "npm run build_static && npm run build_server && npm run build_browser",
    "dev": "cross-env NODE_ENV=development npm run build_static && npm run build_server && node ./bin/main.js",
    "watch": "nodemon --exec 'npm run dev'",
    "lint": "eslint .",
    "fix_lint": "eslint --fix .",
    "postinstall": "cd node_modules/craft-ai && npm install && npm run build",
    "gh_deploy": "./gh-deploy.sh"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://craft.ai/HomeTogether",
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "cpy-cli": "1.0.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.11.1",
    "eslint-config-craft-ai": "2.0.3",
    "eslint-plugin-react": "5.1.1",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "nodemon": "1.9.1",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack-dev-middleware": "1.6.1"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "body-parser": "1.15.0",
    "bootstrap": "3.3.6",
    "craft-ai": "git://github.com/craft-ai/craft-ai-client-js.git#feature/ClientHandleIntervals",
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "font-awesome": "4.5.0",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.6.1",
    "morgan": "1.7.0",
    "node-hue-api": "2.1.1",
    "passport": "0.3.2",
    "passport-oauth": "1.0.0",
    "react": "0.14.7",
    "react-bootstrap": "0.28.3",
    "react-dom": "0.14.7",
    "react-motion": "0.4.2",
    "sync-request": "3.0.1",
    "webpack": "1.13.1",
    "ws": "1.0.1"
  }
}
